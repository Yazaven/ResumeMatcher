{
  "builds": [
      {
          "src": "api/app.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "app.py"
      },
      {
          "src": "(.*)",
          "headers": {"Access-Control-Allow-Origin": "*"},
          "continue": true
      }
  ]
}
